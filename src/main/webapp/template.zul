                    <vlayout>
                        <button label="Home" image="/img/network.gif" width="125px" onClick="@command(&apos;navigateHome&apos;)"/>
                        <button label="Dashboard" image="/img/network.gif" width="125px" onClick="@command(&apos;navigateDashboard&apos;)"/>
